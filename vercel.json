{
  "version": 2,
  "buildCommand": "./build_files.sh",
  "installCommand": "pip install -r requirements.txt",
  "builds": [
    { "src": "app/wsgi.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "app/wsgi.py" }
  ]
}
